{
  "name": "apiario",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nome": {
      "type": "string"
    },
    "localizacao": {
      "type": "string"
    },
    "especie": {
      "type": "string"
    },
    "ativo": {
      "type": "boolean"
    },
    "dataCriacao": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {},
  "mixins": {
    "FireLoop": true,
    "TimeStamp": {
      "required": false
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for apiario registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  }
}