{
  "name": "financeirio",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "FireLoop": true,
    "TimeStamp": {
      "required": false
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for financeirio registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "nome": {
      "type": "string"
    },
    "valor": {
      "type": "number"
    },
    "dataVencimento": {
      "type": "date"
    },
    "dataPagamento": {
      "type": "date"
    },
    "eTituloReceber": {
      "type": "boolean"
    },
    "eTituloQuitado": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "categoria": {
      "type": "belongsTo",
      "model": "financeirioCategoria",
      "foreignKey": ""
    },
    "pessoa": {
      "type": "belongsTo",
      "model": "pessoa",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
