      "type": "number"
    },
    "quantidadeLarva": {
      "type": "number"
    },
    "quantidadePupa": {
      "type": "number"
    },
    "quantidadePupaZangao": {
      "type": "number"
    },
    "quantidadePolen": {
      "type": "number"
    },
    "quantidadeNectar": {
      "type": "number"
    },
    "quantidadeMel": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
